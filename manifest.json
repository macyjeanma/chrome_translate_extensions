{
  "name": "翻譯 單字卡",
  "version": "1.0",
  "permissions": ["activeTab", "storage"],
  "description": "翻譯 單字卡",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "options_page": "review.html"
}
